// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 

 var something =userInput [0];
 if(something % 2 == 0) {
    console.log("Even");
}
else {
    console.log("Odd");
}
  //end-her
});
