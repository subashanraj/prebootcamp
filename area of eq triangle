// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 

var ss = userInput[0];

 var dd = Math.sqrt(3);
 var ll = (Math.floor(dd * ss * ss) / 4).toFixed(2) ;
 console.log(ll);
  //end-here
});
